{
	"name": "svd-style",
	"version": "1.7.0",
	"description": "Style for SVD projects",
	"main": "style.scss",
	"scripts": {
		"start": "rm -rf dist && webpack --config webpack.config.js --watch",
		"test": "npm run settings && npm run test-default && npm run test-full && npm run test-ext && node test/test.js",
		"test-default": "node-sass test/index.scss test/css/default.css",
		"test-ext": "node-sass test/ext.scss test/css/ext.css",
		"test-full": "node-sass test/full.scss test/css/full.css",
		"settings": "node scripts/json-to-css.js",
		"dev": "npm run settings && npm run start --w",
		"build": "npm run settings && rm -rf dist/**/* && npm run build-dist",
		"build-dist": "npm run build-default && npm run build-full",
		"build-default": "node-sass src/scss/style.scss dist/svd.css",
		"build-full": "node-sass src/scss/full.scss dist/svd-full.css",
		"prepublishOnly": "npm run test && npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/silvandiepen/svd-style.git"
	},
	"keywords": [
		"style",
		"assets",
		"digital",
		"styleguide"
	],
	"author": "Sil van Diepen",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/silvandiepen/svd-style/issues"
	},
	"homepage": "https://github.com/silvandiepen/svd-style#readme",
	"devDependencies": {
		"autoprefixer": "^8.0.0",
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.3",
		"babel-preset-env": "^1.6.1",
		"color-convert": "^1.9.1",
		"copy-webpack-plugin": "^4.4.1",
		"css-loader": "^0.28.10",
		"extract-loader": "^1.0.2",
		"extract-text-webpack-plugin": "^3.0.2",
		"file-loader": "^1.1.11",
		"flat": "^4.0.0",
		"grunt": "^1.0.2",
		"html-loader": "^0.5.5",
		"modern-normalize": "^0.4.0",
		"node-sass": "^4.9.0",
		"postcss": "^6.0.19",
		"postcss-loader": "^2.1.1",
		"raw-loader": "^0.5.1",
		"sass-loader": "^6.0.6",
		"style-loader": "^0.20.3",
		"webfonts-loader": "^4.0.1",
		"webpack": "^3.11.0",
		"webpack-cli": "^2.0.13",
		"webpack-dev-server": "2.11.1"
	},
	"dependencies": {
		"sass-true": "^4.0.0",
		"vue": "^2.5.16",
		"vue-loader": "^14.2.2",
		"vue-smooth-scroll": "^1.0.13",
		"vue-template-compiler": "^2.5.16"
	}
}
