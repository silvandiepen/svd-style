{
	"name": "svd-style",
	"version": "1.5.2",
	"description": "Style for SVD projects",
	"main": "index.scss",
	"scripts": {
		"start": "webpack --config webpack.config.js && npm run build",
		"test": "grunt test",
		"settings": "node scripts/json-to-css.js",
		"dev": "npm run settings && node-sass src/scss/style.scss dist/style.css --w",
		"build-css": "node-sass src/scss/style.scss dist/style.css",
		"build": "npm run settings && npm run build-css",
		"prepublishOnly": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/silvandiepen/svd-style.git"
	},
	"keywords": [
		"style",
		"assets",
		"digital",
		"styleguide"
	],
	"author": "Sil van Diepen",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/silvandiepen/svd-style/issues"
	},
	"homepage": "https://github.com/silvandiepen/svd-style#readme",
	"dependencies": {
		"file-loader": "^1.1.11",
		"html-loader": "^0.5.5",
		"pug-html-loader": "^1.1.5"
	},
	"devDependencies": {
		"autoprefixer": "^8.0.0",
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.3",
		"babel-preset-env": "^1.6.1",
		"color-convert": "^1.9.1",
		"copy-webpack-plugin": "^4.4.1",
		"css-loader": "^0.28.10",
		"extract-loader": "^1.0.2",
		"extract-text-webpack-plugin": "^3.0.2",
		"flat": "^4.0.0",
		"grunt": "^1.0.2",
		"modern-normalize": "^0.4.0",
		"node-sass": "^4.8.2",
		"postcss": "^6.0.19",
		"postcss-loader": "^2.1.1",
		"sass-loader": "^6.0.6",
		"webfonts-loader": "^4.0.1",
		"webpack": "^3.11.0"
	}
}
